{
  "name": "marble_project",
  "version": "0.1.0",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "animejs": "^3.0.1",
    "autoprefixer": "^9.4.4",
    "crocks": "^0.11.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-functional-lifecycle": "^3.0.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.2",
    "react-transition-group": "^2.7.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/Style/app.scss -o src/Style/",
    "build-autoprefixed-css": "postcss build/static/css/*.css --replace --use autoprefixer --verbose",
    "watch-css": "node-sass-chokidar --watch --include-path ./node_modules ./src/Style/app.scss -o src/Style/ ",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js build-autoprefixed-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "postcss-cli": "^6.0.0",
    "react-test-renderer": "^16.4.1",
    "stylelint": "^9.10.1",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.5.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
